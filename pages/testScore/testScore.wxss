/* pages/testScore/testScore.wxss */
.body{
  width: 100%;
  height: 100vh;
}
.score{
  display:flex;
  align-items:center;
  justify-content:center;
  flex-direction:column;
  width: 100%;
  height: 60vh;
}
.round{
  width: 300rpx;
  height: 300rpx;
  background-color: var(--blue);
  border-radius: 220rpx;
  display:flex;
  align-items:center;
  justify-content:center;
  box-shadow:2px 2px 20px #909090;    /*背影*/
  color:white;
  font-size: 80rpx;
  font-weight:bold;
  /* border: 10rpx double #909090; */
}
.explain{
  margin-top: 60rpx;
  font-size: 40rpx;
  color: #909090;
}
.button button{
  margin-top: 60rpx;
}